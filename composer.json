{
    "name": "faiznurullah/google-drive-php",
    "description": "A PHP library for Google Drive integration with Laravel-like API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Faiz Nurullah",
            "email": "faizn103a@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "google/apiclient": "^2.18",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "GoogleDrivePHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoogleDrivePHP\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "process-timeout": 900,
        "preferred-install": "dist",
        "archive-format": "zip"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
